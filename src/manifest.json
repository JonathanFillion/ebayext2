{
	"name": "Ebay Sku Generator (BETA)",
	"version": "2.0",
	"description": "Ebay Sku Generator (BETA) by DAPI software",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "assets/icons/icon16.png",
		"19": "assets/icons/icon19.png",
		"38": "assets/icons/icon38.png",
		"48": "assets/icons/icon48.png",
		"96": "assets/icons/icon96.png",
		"128": "assets/icons/icon128.png"

	},
	"permissions": [
		"storage",
		"activeTab",
		"webNavigation"
	],
	"background": {
		"scripts": [
			"assets/js/background.js"
		],
		"persistent": true
	},

	"content_scripts": [
		{
			"matches": [
				"https://bulksell.ebay.ca/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.ca/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://bulksell.ebay.com/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.com/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://bulksell.ebay.co.uk/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.co.uk/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://bulksell.ebay.com.au/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.com.au/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		}
	]
}