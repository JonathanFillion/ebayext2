{
	"name": "Ebay Sku Generator",
	"version": "2.0",
	"description": "Angular Based Sku Generator",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "index.html"
	},
	"permissions": [
		"storage",
		"activeTab",
		"webNavigation"
	],
	"background": {
		"scripts": [
			"assets/js/background.js"
		],
		"persistent": true
	},

	"content_scripts": [
		{
			"matches": [
				"https://bulksell.ebay.ca/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.ca/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://bulksell.ebay.com/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.com/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://bulksell.ebay.co.uk/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.co.uk/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://bulksell.ebay.com.au/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		},
		{
			"matches": [
				"https://www.ebay.com.au/*"
			],
			"js": [
				"assets/js/inject.js"
			]
		}
	]
}